{
  "name": "git-karma",
  "version": "0.1.0",
  "description": "A mutual contribution platform for OSS. Barter your skills, earn Karma, and grow your projects.",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest run",
    "type-check": "tsc --noEmit",
    "postinstall": "prisma generate",
    "migrate": "cross-env PRISMA_MIGRATION=true prisma migrate dev",
    "prepare": "husky",
    "migrate:prod": "cross-env PRISMA_MIGRATION=true prisma migrate deploy"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@prisma/adapter-pg": "^7.0.1",
    "@prisma/client": "^7.0.1",
    "@prisma/config": "^7.0.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "lucide-react": "^0.555.0",
    "next": "^16.0.7",
    "next-auth": "^5.0.0-beta.30",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hot-toast": "^2.6.0",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
